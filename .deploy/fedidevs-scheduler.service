[Unit]
Description=Fedidevs Scheduler
After=fedidevs.service

[Service]
Type=simple
Restart=always
RestartSec=1
WorkingDirectory=/home/anze/projects/fedidevs
Environment="NEW_RELIC_CONFIG_FILE=newrelic.ini"
ExecStart=.venv/bin/newrelic-admin run-program .venv/bin/python manage.py scheduler
User=anze

[Install]
WantedBy=multi-user.target
